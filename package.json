  "private": true,
  "author": "nsarafa",
  "engines": {
    "node": ">=4.3.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "npm-run-all --parallel watch:server watch:build",
